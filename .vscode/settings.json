{
  "css.validate": false,
  "less.validate": false,
  "scss.validate": false,
  "tailwindCSS.includeLanguages": {
    "javascript": "javascript",
    "html": "html",
    "css": "css"
  },
  "files.associations": {
    "*.css": "tailwindcss"
  },
  "editor.quickSuggestions": {
    "strings": true
  },
  "tailwindCSS.experimental.classRegex": [
    ["class[:]\\s*[\"']([^\"']+)[\"']", "([a-zA-Z0-9\\-:]+)"]
  ]
}
